@model IPLAnalytics.Web.ViewModels.ChartViewModel

<div class="container">
    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-heading">@Model.Title Chart</div>
                <div class="panel-body">
                    <div id="chart"></div>
                </div>
            </div>
        </div>
    </div>
</div>
<script>
    $(function() {
        $.getJSON("@Model.DataUrl", function(data) {
            Morris.Bar({
                element: 'chart',
                data: data,
                xkey: 'Over',
                ykeys: ['Run'],
                labels: ['Run'],
                resize: true
                
            });
        });
    });
</script>